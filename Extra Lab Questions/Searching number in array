// WAP to search a given number in an array.
#include <stdio.h>

int main()
{
    int num,n;
    printf("Enter searching number and size of array:");
    scanf("%d%d",&num,&n);
    int a[n];
    printf("Enter array elements:");
    int p=0;
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(int i=0;i<n;i++)
    {
        if(a[i]==num)
        {
            p=i+1;
            break;
        }
        
    }
    if(p!=0)
    printf("Number found at %d position",p);
    else
    printf("Not found");
    

    return 0;
}
