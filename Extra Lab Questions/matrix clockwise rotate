// Rotate an N*N matrix clockwise by 90 degress in place.
#include <stdio.h>

int main() 
{
    int  n;
    printf("Enter size of matrix:");
    scanf("%d",&n);
    int a[n][n];
    printf("Enter array elements:");
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            scanf("%d",&a[i][j]);
        }
        
    }
    
    for(int j=0;j<n;j++)
    {
        for(int i=n-1;i>=0;i--)
        {
          printf("%d",a[i][j]);  
        }
        printf("\n");
    }

    return 0;
}
