// Print all elemnts of a matriz in spiral order.
#include <stdio.h>

int main() 
{
    int  n;
    printf("Enter size of matrix:");
    scanf("%d",&n);
    int a[n][n];
    printf("Enter array elements:");
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            scanf("%d",&a[i][j]);
        }
        
    }
    int i=0;
    for(int j=0;j<n;j++)
    {
        printf("%d , ",a[i][j]);
    }
    int j=n-1;
    for(int i=1;i<n-1;i++)
    {
         printf("%d , ",a[i][j]);
    }
    i=n-1;
    for(int j=n-1;j>=0;j--)
    {
        printf("%d , ",a[i][j]);
    }
    j=0;
    for(int i=n-2;i>0;i--)
    {
          printf("%d , ",a[i][j]);
    }
    
   for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            if(i!=0 && i!=n-1 && j!=0 && j!=n-1)
          printf("%d",a[i][j]);  
        }
    
    }
    return 0;
}
