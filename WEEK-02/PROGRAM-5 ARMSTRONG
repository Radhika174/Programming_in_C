p//Armstrong number
#include <stdio.h>
#include <math.h>
int main()
{
int n;
printf("Enter number");
scanf("%d",&n);
int temp =n;
int c=0;
int sum=0;
int t =n;
while(temp>0)
{
temp=temp/10;
c=c+1;
}
while(n>0)
{
int d=n%10;
sum = sum+pow(d,c);
n=n/10;
}
if(sum==t)
printf("Armstrong");
elserintf("Not armstrong");
return 0;
}
