//Function without argument and return type
#include <stdio.h>
void fact();
int main()
{
printf("Function call starts: \n");
fact();
printf("Task completed \n");
return 0;
}
void fact()
{
int num , f=1;
printf("Enter your number \n");
scanf("%d",&num);
while(num>0)
{
f=f*num;
num=num-1;
}
printf("Factorial=%d",f);
}
