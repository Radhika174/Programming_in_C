//Function without argument and return type
#include <stdio.h>
void fact();
void fib();
int main()
{
printf("Function call starts: \n");
fact();
fib();
printf("Task completed \n");
return 0;
}
void fact()
{
int num , f=1;
printf("Enter your number \n");
scanf("%d",&num);
while(num>0)
{
f=f*num;
num=num-1;
}
printf("Factorial=%d",f);
}
void fib()
{
int a , b ,c ;
printf("Enter a and b");
scanf("%d%d",&a,&b);
printf("%d ",a);
printf("%d ",b);
for(int i=1;i<=10;i++)
{
c=a+b;
printf("%d ",c);
a=b;
b=c;
}
}
