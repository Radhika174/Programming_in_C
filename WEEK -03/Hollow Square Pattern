/*Hollow Square Pattern
 * * * * * * 
 *           *
 *           *
 *           *
 *           *
 * * * * * * 
 */
#include <stdio.h>

int main() 
{
    int n ;
    printf("Enter n");
    scanf("%d",&n);
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=n;j++)
        {
            if(j==1||i==1||i==n||j==n)
             printf("* ");
             else
             printf("  ");
        }
        printf("\n");
    }
   /* for(int i=1;i<=n;i++)
    {
        for(int j=n;j>=i;j--)
        {
             printf("*");
        }
         printf("\n");
    }*/

    return 0;
}
