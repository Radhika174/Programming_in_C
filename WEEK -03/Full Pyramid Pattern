/*Full Pyramid
    * 
   * * 
  * * * 
 * * * * 
* * * * * 

*/
#include <stdio.h>

int main()
{ int m=5;
    for(int i=1;i<=m;i++)
    {
        for(int k=1;k<=m-i;k++)
        {
            printf(" ");
        }
        for(int j=1;j<=i;j++)
        {
            printf("* ");
        }printf("\n");
        
    }
    

    return 0;
}
