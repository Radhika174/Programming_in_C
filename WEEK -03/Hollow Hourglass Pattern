/* Hollow Hourglass Pattern
* * * * * 
 *     * 
  *   * 
   * * 
    * 
    * 
   * * 
  *   * 
 *     * 
* * * * * 

*/
#include <stdio.h>

int main()
{
    int n=5;
    
    for(int i=n;i>=1;i--)
    {
        for(int k=n-1;k>=i;--k)
        {
            printf(" ");
        }
         for (int j=1;j<=i;++j)
         { 
             if(j==1||j==i||i==n)
             printf("* ");
             else
             printf("  ");
             
         }
         printf("\n");
    }
    int m=5;
    for(int i=1;i<=m;i++)
    {
        for(int k=1;k<=m-i;k++)
        {
            printf(" ");
        }
        for(int j=1;j<=i;j++)
        {
            if(j==1||j==i||i==m)
            printf("* ");
            else
            printf("  ");
        }
        printf("\n");
        
    }
    

    return 0;
}
